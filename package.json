{
  "name": "@r00t80y/resize-delay",
  "version": "1.0.1",
  "description": "Create a window resize event with a delay",
  "main": "./dist/lib/ResizeDelay.js",
  "module": "./src/ResizeDelay.js",
  "files": [
    "dist/",
    "src/",
    "LICENSE"
  ],
  "scripts": {
    "server": "http-server ./dist -a localhost -p 9000 -c-1 -o",
    "serve": "cross-env NODE_ENV=development webpack serve --open --config task/webpack.dev.js",
    "build": "npm run build:prod && npm run build:poly",
    "build:prod": "cross-env NODE_ENV=production webpack --config task/webpack.prod.js",
    "build:poly": "cross-env NODE_ENV=production webpack --config task/webpack.prod.polyfills.js",
    "pub": "npm run build && npm version patch && npm publish --access public",
    "lint": "clear && eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/R00T80Y/resize-delay.git"
